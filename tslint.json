{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": [
      true,
      {
        "trailingComma": "es5",
        "printWidth": 100
      }
    ],
    "interface-name": null,
    "jsx-no-lambda": false,
    "no-shadowed-variable": false,
    "radix": null
  }
}
